<?xml version="1.0" encoding="utf-8"?>
<odoo>
		<record id="view_stock_config_settings_export_stock" model="ir.ui.view">
			<field name="name">Sale Order Check Stock Configuration</field>
			<field name="model">res.config.settings</field>
			<field name="inherit_id" ref="sale.res_config_settings_view_form" />
			<field name="priority" eval="20"/>
			<field name="arch" type="xml">
			<data>
				<div class="row mt16 o_settings_container" position="after">
					<div class="row o_settings_container">
						<div class="col-xs-12 col-md-6 o_setting_box">
							<div class="o_setting_left_pane">
								<field name="group_visible_export_stock" string= "Export StockInfo in Excel file ?" class="oe_inline" />
							</div>
							<div class="o_setting_right_pane">
								<label for="group_visible_export_stock"/>
							</div>
						</div>
						<div class="col-xs-12 col-md-6 o_setting_box"/><br></br>
					</div>
				</div>
			</data>
			</field>
		</record>
	
</odoo>